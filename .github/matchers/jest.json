{
  "problemMatcher": [
    {
      "owner": "jest",
      "pattern": [
        {
          "regexp": "^FAIL (.+)$",
          "file": 1
        },
        {
          "regexp": "^  (.+)$",
          "message": 1
        },
        {
          "regexp": "^    (✓|✕) (.+)$",
          "message": 1,
          "loop": true
        }
      ]
    }
  ]
}

